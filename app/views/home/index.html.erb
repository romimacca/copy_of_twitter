<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>
<div class="row">
    <% @tweets.each do |tweet| %>
        <div class>
            <%= image_tag (tweet.user.photo_url), class: "avatar " %>
            <%= tweet.content %>
            <hr/>
            <%= tweet.likes.count %> like
        </div>
    <% end %>

    <%= paginate @tweets %>
</div>